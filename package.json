{
  "name": "mexc-ws-proto",
  "version": "1.0.1",
  "description": "MEXC WebSocket protobuf ES6 library",
  "type": "module",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "build": "pbjs --target static-module --wrap es6 --es6 --out index.js src/*.proto && pbts --out index.d.ts index.js"
  },
  "keywords": [
    "protobuf",
    "mexc",
    "websocket",
    "es6"
  ],
  "author": "GrandF17",
  "license": "MIT",
  "dependencies": {
    "protobufjs": "^7.5.3"
  },
  "devDependencies": {
    "protobufjs-cli": "^1.2.0"
  }
}